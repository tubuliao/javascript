<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="imageList" var="image"  
			varType="com.isoftstone.tyw.entity.info.Image">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0" >image.id</mapping>
				<mapping row="1" col="1" >image.title</mapping>
				<mapping row="1" col="2">image.source</mapping>
				<mapping row="1" col="3">image.personnelTag</mapping>
				<mapping row="1" col="4">image.areaTag</mapping>
				<mapping row="1" col="5">image.knowledgeTag</mapping>
				<mapping row="1" col="6">image.subitemTag</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>