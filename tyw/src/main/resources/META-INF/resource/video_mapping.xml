<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="videoList" var="video"  
			varType="com.isoftstone.tyw.entity.info.Video">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0" >video.id</mapping>
				<mapping row="1" col="1" >video.title</mapping>
				<mapping row="1" col="2">video.source</mapping>
				<mapping row="1" col="3">video.personnelTag</mapping>
				<mapping row="1" col="4">video.areaTag</mapping>
				<mapping row="1" col="5">video.knowledgeTag</mapping>
				<mapping row="1" col="6">video.subitemTag</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>