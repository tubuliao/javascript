<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="thesauresList" var="t"  
			varType="com.isoftstone.tyw.entity.info.Thesaures">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0" >t.id</mapping>
				<mapping row="1" col="1" >t.name</mapping>
				<mapping row="1" col="2">t.len</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>